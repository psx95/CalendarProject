<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlipperCalendar">
        <attr name="fillUpAllDays" format="boolean" />
        <attr name="dateDisplayFormat">
            <enum name="month_year" value="1" />
            <enum name="date_month_year" value="2" />
        </attr>
        <attr name="nextMonthImage" format="integer" />
        <attr name="prevMonthImage" format="integer" />
        <attr name="showSeasonalColorsOnMonths" format="boolean" />
        <attr name="currDateColor" format="reference" />
        <attr name="scrollEnabled" format="boolean" />
    </declare-styleable>
</resources>