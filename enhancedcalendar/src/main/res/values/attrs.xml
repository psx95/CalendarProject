<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlipperCalendar">
        <attr name="fillUpAllDays" format="boolean" />
        <attr name="dateDisplayFormat">
            <enum name="month_year" value="1" />
            <enum name="date_month_year" value="2" />
        </attr>
        <attr name="nextMonthImage" format="reference" />
        <attr name="prevMonthImage" format="reference" />
        <attr name="showSeasonalColorsOnMonths" format="boolean" />
        <attr name="currTopDateColor" format="reference" />
        <attr name="scrollEnabled" format="boolean" />
        <attr name="animateChange" format="boolean" />
        <attr name="animLeftIn" format="reference" />
        <attr name="animLeftOut" format="reference" />
        <attr name="animRightIn" format="reference" />
        <attr name="animRightOut" format="reference" />
        <attr name="specialDayBackground" format="reference" />
        <attr name="specialDayMarkerImage" format="reference" />
        <attr name="specialDayDecorator">
            <enum name="background" value="1" />
            <enum name="markerImager" value="2" />
            <enum name="both" value="3" />
        </attr>
        <attr name="currDateHighlightColor" format="reference" />
    </declare-styleable>
</resources>